%table.table.table-striped.table-hover
  %tr
    %th Establishment
    %th Postal address
    %th Location
    %th From
    %th Through
    %th
  - existences.by_existence_years.each do |existence|
    %tr
      %td
        - if existence.establishment
          = link_to existence.establishment.name, existence.establishment
      %td= existence.address
      %td
        - if existence.locations
          = existence.locations.map(&:address).join(', ')
      %td= existence.start_year
      %td= existence.end_year
